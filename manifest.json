{
    "name": "Hello Extensions",
    "description": "Base Level Extension",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
      "default_popup": "hello.html",
      "default_icon": "hi_icon.png"
    },
    "permissions": [
      "scripting",
      "activeTab"
      
    ],
    "background": {
      "service_worker": "background.js",
      "type": "module"
    },
    "host_permissions": [
      "https://www.instagram.com/*"
    ]
  }